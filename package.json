{
  "name": "resolve-up",
  "description": "Resolve paths to globally installed npm modules using glob patterns. Very fast, great for getting paths to yeoman generators, etc.",
  "version": "0.5.0",
  "homepage": "https://github.com/jonschlinkert/resolve-up",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/resolve-up",
  "bugs": {
    "url": "https://github.com/jonschlinkert/resolve-up/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "array-unique": "^0.2.1",
    "extend-shallow": "^2.0.1",
    "is-valid-glob": "^0.3.0",
    "lazy-cache": "^0.2.4",
    "matched": "^0.3.2",
    "npm-paths": "^0.1.1"
  },
  "devDependencies": {
    "mocha": "*"
  },
  "keywords": [
    "filepath",
    "find",
    "findup",
    "get",
    "glob",
    "global",
    "install",
    "installed",
    "list",
    "match",
    "module",
    "npm",
    "package",
    "path",
    "paths",
    "search"
  ],
  "verb": {
    "related": {
      "list": [
        "look-up",
        "global-paths",
        "global-modules",
        "global-prefix"
      ]
    }
  }
}
